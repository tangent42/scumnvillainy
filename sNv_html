<!--  Template Example
&{template:bitdaction} {{name=@{character_name}}} {{action=doctor}} {{position=Risky}} {{result=[[]]}}
Action
&{template:bitd} {{name=@{character_name}}} {{action=?{Action|doctor|Study|hack|rig|helm|scramble|scrap|skulk|Attune|Command|Consort|Sway}}} {{position=?{Position|Risky|Controlled|Desperate}}} {{effect=?{Effect|Standard|Limited|Great}}} {{result=[[(?{Base Dice|1}+?{Extra Dice|0})d6sk1]]}} 
Fortune
&{template:bitd} {{name=@{character_name}}} {{fortune=?{Testing|Vice|Heat|Tier|Engagement|Gather Info|Attribute|Quality|Magnitude|Sheer Blasted Luck} - {{note=?{Note}}} {{result=[[?{Dice|1}d6sk1]]}} 
Resist
&{template:bitd} {{name=@{character_name}}} {{resist=?{Attribute|Insight|Prowess|Resolve}}} {{stress=[[6-?{How many dice|1}d6sk1]]}} 
&{template:bitd} {{name=Arkady}} {{action=doctor}} {{position=Risky}} {{effect=Standard}} {{result=[[3d6sk1]]}} 
&{template:bitd} {{name=Arkady}} {{fortune=Blasted Damn Fate}} {{note=Note note note note note note}} {{result=[[1d6sk1]]}} 
&{template:bitd} {{name=Arkady}} {{resist=Prowess}} {{stress=[[6-1d6sk1]]}} 
-->

<!-- ROLL TEMPLATES -->


<rolltemplate class="sheet-rolltemplate-bitd">
            <div class="sheet-rolltableholder">
            <div class="sheet-rolltablename">{{charname}}</div>
            <div class="sheet-rolltablerolls">Rolls</div>
            <div class="sheet-rolltabletitle">[{{rolltypetext}}]({{rolltypeimage}})</div>
            <div class="sheet-rolltabledice">{{#rollGreater() roll1value 0}}{{roll1value}}<span class="sheet-comma">, </span>{{/rollGreater() roll1value 0}}{{#rollGreater() roll2value 0}}{{roll2value}}<span class="sheet-comma">, </span>{{/rollGreater() roll2value 0}}{{#rollGreater() roll3value 0}}{{roll3value}}<span class="sheet-comma">, </span>{{/rollGreater() roll3value 0}}{{#rollGreater() roll4value 0}}{{roll4value}}<span class="sheet-comma">, </span>{{/rollGreater() roll4value 0}}{{#rollGreater() bonusdice 0}}{{bonus1}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 0}}{{#rollGreater() bonusdice 1}}{{bonus2}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 1}}{{#rollGreater() bonusdice 2}}{{bonus3}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 2}}{{#rollGreater() bonusdice 3}}{{bonus4}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 3}}{{#rollGreater() bonusdice 4}}{{bonus5}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 4}}{{#rollGreater() bonusdice 5}}{{bonus6}}<span class="sheet-comma">, </span>{{/rollGreater() bonusdice 5}}
                {{#rollLess() roll1value 1}}{{#rollLess() roll2value 1}}{{#rollLess() roll3value 1}}{{#rollLess() roll4value 1}}{{#rollLess() bonusdice 1}}
                <div class="sheet-zerodice1">   {{zerodice1}}, {{zerodice2}}</div>
                <div class="sheet-zerodice2">Zero Dice - take the lowest result above</div>
                {{/rollLess() bonusdice 1}}{{/rollLess() roll4value 1}}{{/rollLess() roll3value 1}}{{/rollLess() roll2value 1}}{{/rollLess() roll1value 1}}
                </div>
            <div  class="sheet-rolltableposition">{{position}}</div><div class="sheet-rolltableeffect">{{effect}}</div>
                </div>
</rolltemplate>

